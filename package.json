{
  "name": "www.blackcatfarms.co",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "Casey Webb <notcaseywebb@gmail.com> (https://caseyWebb.xyz)",
  "license": "WTFPL",
  "scripts": {
    "start": "webpack serve",
    "build": "webpack",
    "preview": "npm run build && http-server dist --port ${PORT:-8080}",
    "lint": "esw --color --ignore-path .gitignore --ext .ts ./",
    "format": "pretty-quick"
  },
  "eslintConfig": {
    "extends": [
      "profiscience"
    ],
    "parserOptions": {
      "project": [
        "./tsconfig.json",
        "./test/tsconfig.json"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format --staged",
      "pre-push": "yarn lint && yarn build --noEmit"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@fontsource/montserrat": "^4.5.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@types/css-minimizer-webpack-plugin": "^3.0.2",
    "@types/mini-css-extract-plugin": "^2.0.1",
    "@types/terser-webpack-plugin": "^5.0.4",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.21.0",
    "eslint-config-profiscience": "^6.0.0",
    "eslint-watch": "^7.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^0.12.3",
    "husky": "^7.0.1",
    "mini-css-extract-plugin": "^2.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.4",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
